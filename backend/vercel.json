{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npx prisma generate && npm run build",
  "startCommand": "npm run start",
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }]
}
