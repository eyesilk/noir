{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "npx prisma generate && npm run build"
      }
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }]
}
